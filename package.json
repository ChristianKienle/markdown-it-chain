{
  "name": "@ckienle/markdown-it-chain",
  "version": "0.1.0",
  "description": "A chaining API like webpack-chain but for markdown-it.",
  "repository": {
    "url": "ChristianKienle/markdown-it-chain",
    "type": "git"
  },
  "main": "src/index.js",
  "files": [
    "src",
    "index.d.ts"
  ],
  "scripts": {
    "test": "npm run lint && jest",
    "lint": "xo src",
    "release": "release-it"
  },
  "author": "ULIVZ <chl814@foxmail.com>",
  "license": "MIT",
  "jest": {
    "testEnvironment": "node"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "webpack-chain": "^6.0.0"
  },
  "keywords": [
    "markdown-it",
    "chain"
  ],
  "engines": {
    "node": ">=6.9"
  },
  "devDependencies": {
    "eslint-plugin-jest": "^23.0.3",
    "eslint-plugin-vue-libs": "^4.0.0",
    "jest-cli": "^24.9.0",
    "markdown-it": "^10.0.0",
    "markdown-it-anchor": "^5.2.5",
    "markdown-it-table-of-contents": "^0.4.4",
    "xo": "^0.28.3",
    "release-it": "12.4.3",
    "babel-preset-env": "^1.7.0",
    "conventional-changelog-cli": "^2.0.25"
  },
  "peerDependencies": {
    "markdown-it": ">=10.0.0"
  },
  "xo": {
    "extends": [
      "plugin:vue-libs/recommended",
      "plugin:jest/recommended"
    ],
    "rules": {
      "unicorn/filename-case": 2
    },
    "envs": [
      "jest"
    ]
  },
  "types": "index.d.ts"
}
